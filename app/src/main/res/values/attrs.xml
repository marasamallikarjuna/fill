    <declare-styleable name="CircleSeekBar">
        <attr name="wheel_max_process" format="integer" />
        <attr name="wheel_cur_process" format="integer" />
        <attr name="wheel_reached_color" format="color" />
        <attr name="wheel_reached_width" format="dimension" />
        <attr name="wheel_reached_has_corner_round" format="boolean"/>
        <attr name="wheel_unreached_color" format="color" />
        <attr name="wheel_unreached_width" format="dimension" />
        <attr name="wheel_pointer_color" format="color" />
        <attr name="wheel_pointer_radius" format="dimension" />
        <attr name="wheel_has_pointer_shadow" format="boolean" />
        <attr name="wheel_has_wheel_shadow" format="boolean" />
        <attr name="wheel_pointer_shadow_radius" format="dimension" />
        <attr name="wheel_shadow_radius" format="dimension" />
        <attr name="wheel_has_cache" format="boolean" />
        <attr name="wheel_can_touch" format="boolean" />
        <attr name="wheel_scroll_only_one_circle" format="boolean" />
    </declare-styleable>

    <!--drop down fab menu style-->

    <declare-styleable name="FabWithLabelView">
        <attr name="android:src" />
        <attr name="srcCompat" />
        <attr name="fabLabel" format="string" />
        <attr name="fabBackgroundColor" format="color" />
        <attr name="fabLabelColor" format="color" />
        <attr name="fabLabelBackgroundColor" format="color" />
        <attr name="fabLabelClickable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpeedDialView">
        <attr name="android:enabled" />
        <attr name="sdExpansionMode" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="sdMainFabAnimationRotateAngle" format="float" />
        <attr name="sdMainFabClosedSrc" format="reference" />
        <attr name="sdMainFabOpenedBg" format="reference" />
        <attr name="sdMainFabClosedIconColor" format="color" />
        <attr name="sdMainFabClosedBackgroundColor" format="color" />
        <attr name="sdMainFabOpenedSrc" format="reference" />
        <attr name="sdMainFabOpenedIconColor" format="color" />
        <attr name="sdMainFabOpenedBackgroundColor" format="color" />
        <attr name="sdOverlayLayout" format="reference" />
        <attr name="sdUseReverseAnimationOnClose" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpeedDialOverlayLayout">
        <attr name="clickable_overlay" format="boolean" />
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean"/>
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="shadow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference"/>
    </declare-styleable>

</resources>